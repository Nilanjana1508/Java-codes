System.out.println(isFishTasty);
}
}
 
public class Boolea
{
public static void main(String[] args)
{
boolean isJavaFun;
boolean isFishTasty;
System.out.println(isJavaFun);
System.out.println(isFishTasty);
}
}
 
17)	Compare 2 integers
public class Compare
{
public static void main(String[] args)
{
int x=10;
int y=9;
System.out.println(x>y);// returns true because 10 is greater than 9
}
}
 
public class Compare
{
public static void main(String[] args)
{
System.out.println(10==15); //returns false because 10 is not equal to 15
}
}
 
18)	If else statements
public class Ifel
{
public static void main(String[] args)
{
if(20>18)
{
System.out.println("20 is greater than 18");
}
else
{
System.out.println("20 is lesser than 18");
}
}
}
 
public class Ifel
{
public static void main(String[] args)
{
int time=20;
if(time<18)
{
System.out.println("Good day.");
}
else
{
System.out.println("Good evening.");
}
}
}
 
public class Ifel
{
public static void main(String[] args)
{
int time=22;
if(time<10)
{
System.out.println("Good morning.");
}
else if(time<20)
{
System.out.println("Good day.");
}
else
{
System.out.println("Good evening.");
}
}
}
 
19)	Switchcase Statements
public class Switca
{
public static void main(String[] args)
{
int day=4;
switch(day)
{
case 1:
System.out.println("Monday");
break;
case 2:
System.out.println("Tuesday");
break;
case 3:
System.out.println("Wednesday");
break;
case 4:
System.out.println("Thursday");
break;
case 5:
System.out.println("Friday");
break;
case 6:
System.out.println("Saturday");
break;
case 7:
System.out.println("Sunday");
break;
default:
System.out.println("Not a valid number to generate day of the week");
break;
}
}
}
 
20)	Loops
public class Loops
{
public static void main(String[] args)
{
int i=0;
while(i<5)
{
System.out.println(i);
i++;
}
}
}
 
public class Loops
{
public static void main(String[] args)
{
int i=0;
do
{
System.out.println(i);
i++;
}
while(i<5);
}
}
 

public class Loops
{
public static void main(String[] args)
{
int i=0;
for(i=0;i<5;i++)
{
System.out.println(i);
}
}
}
 
public class Loops
{
public static void main(String[] args)
{
String[] cars={"Volvo","BMW","Ford","Mazda"};
for(String i:cars)
{
System.out.println(i);
}
}
}
 
public class Loops
{
public static void main(String[] args)
{
for(int i=0;i<10;i++)
{
if(i==4)
{
break;
}
System.out.println(i);
}
}
}
 
public class Loops
{
public static void main(String[] args)
{
for(int i=0;i<10;i++)
{
if(i==4)
{
continue;
}
System.out.println(i);
}
}
}
 
21)	User Input:
import java.util.Scanner;
class Us
{
public static void main(String[] args)
{
Scanner myObj=new Scanner(System.in);
String userName;
System.out.println("Enter username:");
userName=myObj.nextLine();
System.out.println("Username is:"+userName);
}
}
 

22)	Character array to String:
public class StringDemo
{
public static void main(String[] args)
{
char[] helloArray={'h','e','l','l','o','.'};
String helloString= new String(helloArray);
System.out.println(helloString);
}
}
 
23)	Calculator:
import java.util.Scanner;
public class Calculator{
public static void main(String []args){
        int c;
        do
        {
            System.out.println("1. Addition");
            System.out.println("2. Subtraction");
            System.out.println("3. Multiplication");
            System.out.println("4. Division");
            System.out.println("5. Modulus");
            System.out.println("6. Exponentiation");
            Scanner myObj=new Scanner(System.in);
            System.out.println("Enter your choice of arithmetic operation to be performed:");
            int choice=myObj.nextInt();
            System.out.println("Enter first number:");
            int n1=myObj.nextInt();
            System.out.println("Enter second number:");
            int n2=myObj.nextInt();
            switch(choice)
            {
                case 1:
                    int n3= n1+n2;
                    System.out.println("The answer is:");
                    System.out.println(n3);
                    break;
                case 2:
                    int n4= n1-n2;
                    System.out.println("The answer is:");
                    System.out.println(n4);
                    break;
                case 3:
                    int n5= n1*n2;
                    System.out.println("The answer is:");
                    System.out.println(n5);
                    break;
                case 4:
                    int n6= n1/n2;
                    System.out.println("The answer is:");
                    System.out.println(n6);
                    break;
                case 5:
                    int n7= n1%n2;
                    System.out.println("The answer is:");
                    System.out.println(n7);
                    break;
                case 6:
                    double n8= Math.pow(n1,n2);
                    System.out.println("The answer is:");
                    System.out.println(n8);
                    break;
                default:
                    System.out.println("Invalid choice!");
            }
            System.out.println("Do you want to continue? Press 0 to stop or 1 to continue:");
            c = myObj.nextInt();
        }
        while(c==1);
    }
}
 

